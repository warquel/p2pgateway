<% if @zyre_bind -%>
ZYRE_BIND="<%= scope['p2p_gateway::zyre_proto'] %>://<%= scope['p2p_gateway::zyre_ip'] %>:<%= scope['p2p_gateway::zyre_port'] %>"
<% end -%>
<% if scope['p2p_gateway::private_key_path'] -%>
PRIVATE_KEY_PATH=<%= scope['p2p_gateway::private_key_path'] %>
<% end -%>
<% if scope['p2p_gateway::public_key_dir_path'] -%>
PUBLIC_KEY_DIR_PATH=<%= scope['p2p_gateway::public_key_dir_path'] %>
<% end -%>
<% if scope['p2p_gateway::disable_curve'] -%>
DISABLE_CURVE=1
<% end -%>
<% if @pubsub_bind -%>
PUBSUB_ENDPOINT="<%= scope['p2p_gateway::pubsub_proto'] %>//<%= scope['p2p_gateway::pubsub_ip'] %>:<%= scope['p2p_gateway::pubsub_port'] %>"
<% end -%>
<% if @control_bind -%>
CONTROL_ENDPOINT="<%= scope['p2p_gateway::control_proto'] %>//<%= scope['p2p_gateway::control_ip'] %>:<%= scope['p2p_gateway::control_port'] %>"
<% end -%>
